#include <stdio.h>
#include "variadic_functions.h"
#include <stdarg.h>/**
 *print_strings - function that prints numbers, followed by a new line
 *@separator : parameter for separators
 *@n : parameter if numbers
 *Return: nothing.
 */
print_letter(arg, va_list)
{
printf("%c", s);
}
print_flo(arg, va_list...)
{
printf("%f",n);
}
print_number(int a)
{
printf("%d", a);
}
print_str(char *s)
{
if (s == NULL)
printf("(nil)");
printf("%s", s);
}
